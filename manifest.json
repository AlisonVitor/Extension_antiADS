{
    "manifest_version": 3,
    "name": "Antispam Ad Blocker - AMVB",
    "version": "1.0",
    "description": "Extensão para bloquear anúncios indesejados.",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["styles.css"]
      }
    ],
    "permissions": [
      "activeTab"
    ]
  }
  